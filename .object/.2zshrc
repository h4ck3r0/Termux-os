ZSH_THEME="h4Ck3r"
export ZSH=$HOME/.oh-my-zsh
plugins=(git)

source $HOME/.oh*/oh-my-zsh.sh
source /data/data/com.termux/files/home/.oh-my-zsh/plugins/zsh-autosuggestions/zsh-autosuggestions.zsh
source /data/data/com.termux/files/home/.oh-my-zsh/plugins/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
# User configuration


#figlet -f ASCII-Shadow "  h4ck3r" | lolcat -t
clear

draw_banner() {
    # Define colors
    cyan='\033[0;36m'
    reset='\033[0m'
    
    # Top of the Box
    echo -e "${cyan} ╔════════════════════════════════════════════╗${reset}"
    echo -e "${cyan} ║                                            ║${reset}"
    
    # Big Text "PROC" (Rainbow Color)
    figlet -f ASCII-Shadow "\PROC" | lolcat
    
    # Info Section
    echo -e "${cyan} ║                                            ║${reset}"
    echo -e "${cyan} ║        SYSTEM: ONLINE  |  USER: ROOT       ║${reset}"
    echo -e "${cyan} ║        OS    : Termux-OS v3                ║${reset}"
    echo -e "${cyan} ║        SECURE CONNECTION ESTABLISHED       ║${reset}"
    echo -e "${cyan} ║                                            ║${reset}"
    echo -e "${cyan} ╚════════════════════════════════════════════╝${reset}"
}

# Run the banner
draw_banner
PUT 4 0
figlet -f ASCII-Shadow "\PROC" | lolcat -t
PUT 4 0
echo -e "\033[33m │"
PUT 5 0
echo -e "\033[33m │"
PUT 6 0
echo -e "\033[33m │"
PUT 7 0
echo -e "\033[33m │"
PUT 8 0
echo -e "\033[33m │"
PUT 9 0
echo -e "\033[33m │"
PUT 10 0
echo -e "\033[33m │"
PUT 13 0
